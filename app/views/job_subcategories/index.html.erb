<p id="notice"><%= notice %></p>

<h1>Listing Job Subcategories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_subcategories.each do |job_subcategory| %>
      <tr>
        <td><%= job_subcategory.name %></td>
        <td><%= link_to 'Show', job_subcategory %></td>
        <td><%= link_to 'Edit', edit_job_subcategory_path(job_subcategory) %></td>
        <td><%= link_to 'Destroy', job_subcategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job subcategory', new_job_subcategory_path %>
