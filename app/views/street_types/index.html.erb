<p id="notice"><%= notice %></p>

<h1>Listing Street Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @street_types.each do |street_type| %>
      <tr>
        <td><%= street_type.name %></td>
        <td><%= link_to 'Show', street_type %></td>
        <td><%= link_to 'Edit', edit_street_type_path(street_type) %></td>
        <td><%= link_to 'Destroy', street_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Street type', new_street_type_path %>
